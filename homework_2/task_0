#include <stdio.h>

int main() {
	int num, a, b, c, d, summ, prod;
    scanf("%i", &num);
	a = num % 10;
	b = num % 100 / 10;
	c = num % 1000 / 100;
	d = num / 1000;
	summ = a + b + c + d;
	prod = a * b * c * d;
	printf("Сумма цифр: %i\n", summ);
	printf("Произведение цифр: %i\n", prod);
	printf("Сумма цифр в восьмеричной: %o\n", summ);
	printf("Произведение цифр в шестандатеричной: %x\n", prod);
}
