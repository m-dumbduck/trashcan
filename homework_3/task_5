#include <stdio.h>
#include <math.h>

int main() {
    int n;
	printf("Введите количество апельсинов: ");
	scanf("%d", &n);
	if ((n % 100 >= 11 && n % 100 <= 19) || n % 10 > 4 || n % 10 == 0) {
		printf("%d апельсинов", n);
	} else if (n % 10 == 1) {
		printf("%d апельсин", n);
	} else {
		printf("%d апельсина", n);
	}
}
